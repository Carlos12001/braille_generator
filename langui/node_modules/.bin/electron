#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/diabl/OneDrive/Documents/Code/Electron/langui/node_modules/.pnpm/electron@24.3.1/node_modules/electron/node_modules:/mnt/c/Users/diabl/OneDrive/Documents/Code/Electron/langui/node_modules/.pnpm/electron@24.3.1/node_modules:/mnt/c/Users/diabl/OneDrive/Documents/Code/Electron/langui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/diabl/OneDrive/Documents/Code/Electron/langui/node_modules/.pnpm/electron@24.3.1/node_modules/electron/node_modules:/mnt/c/Users/diabl/OneDrive/Documents/Code/Electron/langui/node_modules/.pnpm/electron@24.3.1/node_modules:/mnt/c/Users/diabl/OneDrive/Documents/Code/Electron/langui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../electron/cli.js" "$@"
else
  exec node  "$basedir/../electron/cli.js" "$@"
fi
